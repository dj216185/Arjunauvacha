{
  "version": 2,
  "functions": {
    "api/app.py": {
      "runtime": "python3.11",
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "routes": [
    { "src": "^/(api|uploads)(/.*)?$", "dest": "api/app.py" },
    { "src": "^/static/(.*)$", "dest": "static/$1" },
    { "src": "^/(favicon\\.ico|robots\\.txt)$", "dest": "/$1" },
    { "src": "^/(.*)$", "dest": "index.html" }
  ]
}
